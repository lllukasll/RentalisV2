@model IEnumerable<Rentalis_v2.Models.BookingModels>
@{
    ViewBag.Title = "Bookings";
    Layout = "~/Views/Shared/_ManageLayout.cshtml";
}
<style>
    .sidenav {
        height: 100%;
        width: 200px;
        position: fixed;
        z-index: 1;
        top: 0;
        left: 0;
        background-color: #111;
        overflow-x: hidden;
        padding-top: 100px;
    }

    .sidenav a {
        padding: 6px 6px 6px 32px;
        text-decoration: none;
        font-size: 25px;
        color: #818181;
        display: block;
    }

    .sidenavElement {
        padding: 6px 6px 6px 32px;
        text-decoration: none;
        font-size: 25px;
        color: #818181;
        display: block;
    }

    .sidenavElement:hover{
        color: #f1f1f1;
    }
    .sidenav a:hover {
        color: #f1f1f1;
    }

    .sidenavMainPage {
        margin-left: 200px;
    }
</style>
<div class="sidenav">
    @Html.ActionLink("Moj profil", "Index", null, new { @class = "sidenavElement" })
    @Html.ActionLink("Rezerwacje","Bookings",null,new {@class="sidenavElement"})
    <a href="#">Clients</a>
    <a href="#">Contact</a>
</div>
<div class="sidenavMainPage top-margin gtco-container">
    <h2>Rezerwacje</h2>

    <table id="tableAnnounce" class="table table-advance table-hover">
        <tbody>
            <tr>
                <th>CarID</th>
                <th>DateFrom</th>
                <th>DateTo</th>
                <th>TotalPrice</th>
                <th></th>
            </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.carId)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.DateTimeFrom)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.DateTimeTo)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.TotalPrice)
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>